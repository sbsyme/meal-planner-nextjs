        {
          "meal_name": "Scrambled Eggs on Toast",
          "recipe": {
            "steps": [
              "Crack 2 eggs into a bowl, add a pinch of salt and pepper, and whisk until combined.",
              "Heat a non-stick pan over medium heat and add a small knob of butter.",
              "Once the butter has melted, pour the eggs into the pan.",
              "Stir gently with a spatula until the eggs are softly set and slightly runny in places.",
              "Toast two slices of bread in a toaster.",
              "Serve the scrambled eggs on the toasted bread."
            ],
            "ingredients": [
              {
                "ingredient_name": "Tesco Free Range Eggs",
                "quantity": "2 eggs"
              },
              {
                "ingredient_name": "Tesco Everyday Value White Bread",
                "quantity": "2 slices"
              },
              {
                "ingredient_name": "Tesco Butter",
                "quantity": "10g"
              }
            ]
          }
        },
        {
          "meal_name": "Chicken Salad",
          "recipe": {
            "steps": [
              "Dice 200g of chicken breast.",
              "Heat a pan over medium heat and add a teaspoon of olive oil.",
              "Cook the chicken pieces until golden brown and cooked through, about 8 minutes.",
              "Chop 1 carrot and 1/4 onion finely.",
              "In a bowl, combine the cooked chicken, chopped carrot, and onion.",
              "Add a handful of frozen peas and a tablespoon of olive oil.",
              "Mix well and serve."
            ],
            "ingredients": [
              {
                "ingredient_name": "Tesco British Chicken Breast Portions",
                "quantity": "200g"
              },
              {
                "ingredient_name": "Tesco Carrots",
                "quantity": "1 carrot"
              },
